  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "knex": "3.1.0",
    "nodemon": "3.1.7",
    "sqlite3": "5.1.7",
    "uuid": "11.0.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "ts-node-dev": "2.0.0",
    "typescript": "5.6.3"
  }
}
